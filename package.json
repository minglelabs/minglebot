{
  "name": "minglebot",
  "version": "0.1.0",
  "private": true,
  "description": "Local-first personal data import hub for AI agents",
  "main": "dist/server.js",
  "scripts": {
    "build": "vite build && tsc -p tsconfig.json",
    "build:web": "vite build",
    "dev": "tsx watch src/server.ts",
    "dev:web": "vite",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit -p tsconfig.json && tsc --noEmit -p web/tsconfig.json",
    "test": "tsx --test test/**/*.test.ts"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.2",
    "playwright": "^1.58.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.4",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "autoprefixer": "^10.4.24",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^7.3.1"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.15.4"
}
